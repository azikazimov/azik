#Парсер погодного сайта для СПб

import urllib.request
import requests
from bs4 import BeautifulSoup

site = input('Введите ссылку на сайт без HTTPS://')

source = requests.get('https://' + site)
main_text = source.text

soup = BeautifulSoup(main_text, features='html.parser')

span = soup.find('span', {'class': 'temp__value'})
span = span.text

div = soup.find('div', {'class': 'link__condition day-anchor i-bem'})
div = div.text

def dwn_img(url):
    name = 'weather.jpg'
    urllib.request.urlretrieve(url, name)

weather = open('weather.txt', 'w')
weather.write('Температура : ' + span + '\n')
weather.write('Погода : ' + div + '\n')

weather.close()

if div.title() == 'Пасмурно':
    dwn_img('data:image/png;base64,')
elif div.title() == 'Ясно':
    dwn_img('data:image/png;base64,')

#Отладка
print(span)
print(div)
